{
  "name": "serverless-github-api",
  "version": "1.0.0",
  "description": "Serverless GitHub Wrapper for pull requests",
  "bin": {
    "app": "cdk/bin/app.js"
  },
  "main": "src/server.ts",
  "scripts": {
    "start": "nodemon",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "rimraf dist/ && tsc -p ./",
    "test": "jest --detectOpenHandles",
    "cdk": "cdk",
    "build:app": "npm ci && npm run build",
    "cdk:deploy": "npm run cdk deploy",
    "cdk:diff": "npm run cdk diff",
    "cdk:synth": "npm run cdk synth"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/austinloveless/serverless-github-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/austinloveless/serverless-github-api/issues"
  },
  "homepage": "https://github.com/austinloveless/serverless-github-api#readme",
  "devDependencies": {
    "@types/aws-serverless-express": "^3.3.5",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.4",
    "@types/supertest": "^2.0.11",
    "aws-cdk": "^1.129.0",
    "dotenv": "^10.0.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.129.0",
    "@aws-cdk/aws-lambda": "^1.129.0",
    "@aws-cdk/aws-secretsmanager": "^1.129.0",
    "@aws-cdk/core": "^1.129.0",
    "aws-serverless-express": "^3.4.0",
    "axios": "^0.23.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "source-map-support": "^0.5.20",
    "zod": "^3.10.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "."
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.module.(t|j)s"
    ],
    "testEnvironment": "node"
  }
}
